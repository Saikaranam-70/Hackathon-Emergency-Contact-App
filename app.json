{
  "expo": {
    "name": "Emergency",
    "slug": "Emergency",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/back.png",
    "scheme": "emergency",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show your current position on the map.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to show your current position on the map."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyCm_jC3d7tmRDskhymD3Td3uavUNTvlEAs"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.saikaranam_123.Emergency",
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCm_jC3d7tmRDskhymD3Td3uavUNTvlEAs"
        }
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/back.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0da134a8-85a8-4ccd-a1a5-b40b2821bf5d"
      }
    }
  }
}
